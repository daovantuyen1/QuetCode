@{
    ViewBag.Title = "Home Page";
}

<div>
    <button id="btnck">Check api valid</button>
</div>
@section scripts
{
 <script>
     $(function () {
         $("#btnck").click(() => {
             $.ajax({
                 url: "http://localhost:51763/api/MainApi/PostTest",
                 method: "POST",
                 async: false,
                 dataType: "json",
                 contentType:"application/json; charset=utf-8",
                 headers: {
                     Authorization: "Bearer " + Cookies.get('SessionToken')
                 },
                 data: JSON.stringify({ Name: 'tuyen', Age: 1, Phone: "asas" }),
                 success: (dat) => { console.log(dat); },
                 error: (er) => { console.log(er); }
             });

         });
     });

    
    </script>
    
    }